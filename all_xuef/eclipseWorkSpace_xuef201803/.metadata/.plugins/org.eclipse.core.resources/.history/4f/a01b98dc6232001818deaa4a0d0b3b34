package com.xuef201803.book_dp.maze;

public class Door extends MapSite {
	/**
	 * 程序中的Door 直击门的本质
	 * 让你思考: 什么是一扇门
	 */
	private Integer roomA;
	private Integer roomB;
	public Door(Integer roomA, Integer roomB){
		this.roomA = roomA;
		this.roomB = roomB;
	}
	
	public boolean isOpen(){
		return false;
	}
}
