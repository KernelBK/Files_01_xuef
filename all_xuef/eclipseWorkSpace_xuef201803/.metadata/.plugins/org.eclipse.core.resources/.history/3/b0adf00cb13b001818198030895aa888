package multi_thread.thread.runnable.strategy;
/**
 * 用个人所得税计算器的设计来说明Runnable接口的设计
 * Target: 纳税人 
 * @author moveb
 */
public class Target {
	private double salary;
	private double bonus;
	
	// 计算所得税的策略
	private TaxCalcStrategy taxCalcStrategy;
	public Target() {
		this.salary = salary;
		this.bonus = bonus;
	}
	
	
}
