package com.xuef201803.book_dp.singleton;

public class LazyInitSingleton {
	private static LazyInitSingleton singleton = null;
	// 构造器 私有化
	private LazyInitSingleton(){}
	
	public synchronized static LazyInitSingleton newInstance(){
		if(singleton==null){
			singleton = new LazyInitSingleton();
			return singleton;
		}
		return singleton;
	}
}
